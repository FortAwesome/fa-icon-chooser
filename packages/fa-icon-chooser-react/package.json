    "react-dom": "^16.13.1",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "@fortawesome/fa-icon-chooser": "^0.0.1"
  },
  "peerDependencies": {
    "react": "^16.13.0",
    "react-dom": "^16.13.0"
